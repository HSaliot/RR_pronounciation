<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="vBoxHP" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
<children>
    <Accordion VBox.vgrow="ALWAYS">
    <panes>
        <TitledPane animated="false" text="Reading Selection">
            <content>
                <VBox>
                    <children>
                        <HBox alignment="CENTER_RIGHT">
                           <children>
                              <Button fx:id="btnHAddSelection" mnemonicParsing="false" text="Add" />
                           </children>
                        </HBox>
                        <TilePane fx:id="tpReadingSelections" styleClass="tp" />
                    </children>
                </VBox>
            </content>
        </TitledPane>
        <TitledPane animated="false" text="Students">
               <content>
                  <VBox>
                    <children>
                        <TilePane fx:id="tpEvaluatStudents" styleClass="tp" />
                    </children>
                  </VBox>
               </content>
          </TitledPane>
          <TitledPane animated="false" text="Evaluations">
               <content>
                  <VBox>
                    <children>
                        <TilePane fx:id="tpEvaluations" styleClass="tp" />
                    </children>
                  </VBox>
               </content>
          </TitledPane>
        </panes>
    </Accordion>
</children>
</VBox>
